# DeepSeek OCR CLI Configuration (Ollama Backend)
# Copy this file to .env and customize as needed

# Ollama configuration
DEEPSEEK_OCR_MODEL_NAME=deepseek-ocr
DEEPSEEK_OCR_OLLAMA_URL=http://localhost:11434

# Output configuration
DEEPSEEK_OCR_OUTPUT_DIR=output
DEEPSEEK_OCR_EXTRACT_IMAGES=false
DEEPSEEK_OCR_INCLUDE_METADATA=true

# Logging
DEEPSEEK_OCR_LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL
DEEPSEEK_OCR_VERBOSE=false
